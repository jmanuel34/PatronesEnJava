          <ownedParameter xmi:id="_81YOU-qoEeqaHP7rPiRNIA" name="motor">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_81YOVOqoEeqaHP7rPiRNIA" type="_80kWAOqoEeqaHP7rPiRNIA" direction="return"/>
          <language>java</language>
          <body>if (motor == null) {
			return new EmptyConnection();
		}
		else if (motor.equalsIgnoreCase(&quot;ORACLE&quot;)) {
			return new OracleConnection();
		}
		else if (motor.equalsIgnoreCase(&quot;MYSQL&quot;)) {
			return new MySqlConnection();
		}
		else if (motor.equalsIgnoreCase(&quot;POSTGRESQL&quot;)) {
			return new PostgreSqlConnection();
		}
		return new EmptyConnection();
	</body>
        </ownedBehavior>
